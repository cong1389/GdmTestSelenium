        </step>


        <step controltype="container" action="click" controlid="div1" controlvalue="View style options"></step>
        <step controltype="checkbox" action="tick" controlid="FIELD_871" controlvalue="true"></step>

        <step controltype="image" action="upload" controlid="DIV_482" controlvalue="adidas.png">
          <expectations>
            <expectation asserttype="isdisplaynotification" expectedvalue="" assertmessage="It is wrong"></expectation>
            <expectation asserttype="equallableimage" expectedvalue="adidas.png" assertmessage="Upload files before and after are not the same"></expectation>
